<?xml version="1.0"?>
<package format="2">
    <name>rrt_local_planner</name>
    <version>0.1.0</version>
    <description>OMPL based RRT local planner</description>
    <maintainer email="Phillip.Haeusler@boeing.com">Phillip Haeusler</maintainer>
    <author>Boeing</author>
    <license>Boeing Proprietary</license>

    <buildtool_depend>catkin</buildtool_depend>

    <depend>costmap_2d</depend>
    <depend>geometry_msgs</depend>
    <depend>navigation_interface</depend>
    <depend>nav_msgs</depend>
    <depend>pluginlib</depend>
    <depend>roscpp</depend>
    <depend>tf2_ros</depend>
    <depend>ompl</depend>
    <depend>rviz_visual_tools</depend>

    <export>
        <navigation_interface plugin="${prefix}/plugin.xml"/>
    </export>
</package>
